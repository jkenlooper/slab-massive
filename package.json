{
  "name": "slab-massive",
  "version": "0.1.2",
  "description": "Web Component for displaying and navigating partial content",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "css-byebye": "^1.0.1",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.1",
