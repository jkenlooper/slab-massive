{
  "name": "slab-massive",
  "version": "0.1.2",
  "description": "Web Component for displaying and navigating partial content",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.2",
    "cssnano": "^3.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "postcss-calc": "^5.2.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1",
    "raw-loader": "^0.5.1",
    "standard": "^6.0.8",
    "style-loader": "^0.13.1",
    "svgo-loader": "^1.1.2",
    "webpack": "^2.1.0-beta.25"
  },
  "scripts": {
    "pretest": "standard",
    "prebuild": "rm -rf dist; mkdir -p dist",
    "build": "make && mv src/slab-massive.tmp.js dist/slab-massive.js",
    "postbuild": "git update-index --no-assume-unchanged `git ls-files dist/`",
    "preinstall": "npm run build",
    "predebug": "git update-index --assume-unchanged `git ls-files dist/`",
    "debug": "npm run build",
    "watch": "npm run debug -- --watch",
    "preversion": "npm test && npm run build",
    "version": "git add -f dist && git commit -m 'Add dist/ files' || echo 'No change to dist/ files'"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist"
    ]
  },
  "main": "dist/slab-massive.js",
  "author": "Jake Hickenlooper",
  "license": "MIT",
  "dependencies": {}
}
