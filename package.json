{
  "name": "slab-massive",
  "version": "0.0.0",
  "description": "Web Component for displaying and navigating partial content",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "css-byebye": "^1.0.1",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.1",
    "cssnano": "^3.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "postcss-calc": "^5.2.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1",
    "raw-loader": "^0.5.1",
    "standard": "^6.0.8",
    "style-loader": "^0.13.1",
    "svgo-loader": "^1.1.0",
    "webpack": "^1.13.0"
  },
  "scripts": {
    "pretest": "standard",
    "prebuild": "rm -rf dist",
    "build": "webpack --optimize-minimize --optimize-occurence-order",
    "postbuild": "git update-index --no-assume-unchanged `git ls-files dist/`",
    "preinstall": "npm run build",
    "predebug": "git update-index --assume-unchanged `git ls-files dist/`",
    "debug": "webpack --debug --devtool source-map --output-pathinfo",
    "watch": "npm run debug -- --watch",
    "preversion": "npm test && npm run build",
    "version": "git add -f dist && git commit -m 'Add dist/ files' || echo 'No change to dist/ files'"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist",
      "webpack.config.js"
    ]
  },
  "main": "dist/slab-massive.js",
  "author": "Jake Hickenlooper",
  "license": "MIT",
  "dependencies": {
    "hammerjs": "file:lib/hammer.js-2.0.8.tar.gz",
    "webcomponents.js": "file:lib/webcomponentsjs-0.7.22.tar.gz"
  }
}
