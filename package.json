{
  "name": "slab-massive",
  "version": "0.2.0",
  "description": "Web Component for displaying and navigating partial content",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-webpack": "^6.4.3",
    "css-loader": "^0.28.0",
    "css-to-string-loader": "^0.1.2",
    "cssnano": "^3.5.2",
    "csssources": "0.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-minifier": "^3.1.0",
    "postcss-calc": "^5.2.1",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1",
    "raw-loader": "^0.5.1",
    "standard": "^6.0.8",
    "style-loader": "^0.13.1",
    "svgo-loader": "^1.1.2",
    "uglify-js": "https://github.com/mishoo/UglifyJS2/tarball/harmony",
    "webpack": "2.3.3"
  },
  "scripts": {
    "start": "webpack --config webpack.config.js",
    "minifycss-prerequisites": "csssources src/slab-massive.css > .minifycss-prerequisites",
    "minifycss": "postcss -c postcss.config.js -o src/slab-massive.tmp.css src/slab-massive.css",
    "minifyhtml": "html-minifier --case-sensitive --html5 --remove-attribute-quotes --remove-comments --remove-tag-whitespace --collapse-whitespace -o src/slab-massive.tmp.html src/slab-massive.html",
    "minifyjs": "uglifyjs src/slab-massive.tmp.js --wrap slabMassive -c -m --keep-fnames --screw-ie8 -o dist/slab-massive.min.js",
    "pretest": "standard",
    "prebuild": "rm -rf dist; mkdir -p dist",
    "build": "make",
    "postbuild": "git update-index --no-assume-unchanged `git ls-files dist/`",
    "prepublish": "npm run build",
    "predebug": "git update-index --assume-unchanged `git ls-files dist/`",
    "debug": "npm run build",
    "preversion": "npm test && npm run build",
    "version": "git add -f dist && git commit -m 'Add dist/ files' || echo 'No change to dist/ files'"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist",
      "tests",
      "src/*.tmp*",
      "src/_*.js",
      "webpack.config.js"
    ]
  },
  "main": "dist/slab-massive.min.js",
  "author": "Jake Hickenlooper",
  "license": "MIT",
  "dependencies": {}
}
